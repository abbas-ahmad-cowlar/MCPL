model "Test_Arrays_Only"
  uses "mmsystem"
  
  declarations
    nI = 4
    nJ = 8
  end-declarations
  
  declarations
    FACILITIES: set of integer
    CUSTOMERS: set of integer
    COST: array(0..nI-1) of real
    DEMAND: array(0..nJ-1) of real
  end-declarations
  
  initializations from "data/test_tiny.dat"
    FACILITIES CUSTOMERS
    COST DEMAND
  end-initializations
  
  writeln("Successfully loaded!")
  writeln("FACILITIES: ", FACILITIES)
  writeln("CUSTOMERS: ", CUSTOMERS)
  writeln("COST: ", COST)
  writeln("DEMAND: ", DEMAND)
  
end-model
